    <validationRules>
        <fullName>Procedure_Scheduler_Account_Only</fullName>
        <active>true</active>
        <description>This forces the user to only select &quot;Account&quot; in reference to the &apos;Related To&apos; field on the &apos;Procedure Scheduler&apos; record type.  Assignment functionality depends on it.</description>
        <errorConditionFormula>AND(
(RecordType.Name = &quot;Procedure Scheduler&quot;),
(LEFT(WhatId,3) &lt;&gt; &quot;001&quot;))</errorConditionFormula>
        <errorMessage>For this event you must select &quot;Account&quot; from the &apos;Related To&apos; list.</errorMessage>
    </validationRules>
