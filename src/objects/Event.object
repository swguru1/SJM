        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)} 
var newRecords = []; 
var e = new sforce.SObject(&quot;Event&quot;); 
e.id =&quot;{!Event.Id}&quot;;
