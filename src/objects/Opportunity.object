    <validationRules>
        <fullName>IL_FirstUse_Install_Close_Date</fullName>
        <active>true</active>
        <description>If the Opportunity Type is Integrated Lab, then Close date &lt; installation date &lt; first use date</description>
        <errorConditionFormula>AND($RecordType.Name = &quot;Integrated Lab&quot;,OR(First_Use_Date__c &lt; Installed_Date__c,Installed_Date__c &lt; CloseDate, First_Use_Date__c &lt; CloseDate))</errorConditionFormula>
        <errorMessage>Please validate Close Date, Install Date and First Use Date. Install Date and First Use Date should be greater than Close Date. First Use Date should be greater than Install Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Opportunity_Prospecting</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() , NOT( ISPICKVAL( StageName  , &quot;Prospecting&quot;)), NOT($RecordType.DeveloperName= &quot;CardioMEMS_Reorder&quot;))</errorConditionFormula>
        <errorMessage>Opportunity Created new should have a Stage as &quot;Prospecting&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Edit_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(PRIORVALUE( StageName ), &quot;Closed Won&quot;),
NOT(OR(
 $Profile.Name = &apos;System Administrator&apos;
)
)
)</errorConditionFormula>
        <errorMessage>Opportunity closed  Won cannot be re-opened by a field representative. Please select Clone Option from Opportunity Detail screen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Owner_Edit</fullName>
        <active>true</active>
        <errorConditionFormula>AND((Owner.Id  &lt;&gt;  $User.Id), ISCHANGED( StageName ), $UserRole.Name &lt;&gt; &apos;Customer Service - Inside Sales&apos;)</errorConditionFormula>
        <errorMessage>Only Opportunity Owner Can update</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR
(ISPICKVAL(StageName,&quot;Qualifying&quot;),
ISPICKVAL(StageName,&quot;Handling Objections&quot;),
ISPICKVAL(StageName,&quot;Final Negotiations&quot;),
ISPICKVAL(StageName,&quot;Closed Won&quot;)

)


,HasOpportunityLineItem=FALSE,
$RecordType.Name = &quot;SJM Capital Sales&quot;
)</errorConditionFormula>
        <errorMessage>Products must be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Lost_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(IsClosed = TRUE,
IsWon = FALSE,
ISBLANK(TEXT((Reason_Lost__c))))</errorConditionFormula>
        <errorDisplayField>Reason_Lost__c</errorDisplayField>
        <errorMessage>Reason Lost is required for Opportunity with a Stage of &quot;Closed  Lost&quot;</errorMessage>
    </validationRules>
